{
  "name": "chat-bot-v1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:widget": "webpack --config webpack.embed.config.js",
    "build:widget:dev": "webpack --config webpack.embed.config.js --mode development",
    "build:embed": "webpack --config webpack.embed.config.js",
    "build:embed:dev": "webpack --config webpack.embed.config.js --mode development",
    "build:all": "npm run build && npm run build:widget",
    "vercel-build": "npm run build:all",
    "serve:widget": "npm run build:widget:dev && python -m http.server 8080 --directory dist/embed",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "vite": "^7.0.4"
  }
}
