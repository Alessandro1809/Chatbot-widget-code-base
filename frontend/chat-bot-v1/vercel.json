{
  "version": 2,
  "name": "chat-widget-app",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/chat-widget.js",
      "dest": "/embed/chat-widget.js"
    },
    {
      "src": "/chat-widget.css",
      "dest": "/embed/chat-widget.css"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
