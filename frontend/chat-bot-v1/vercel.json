{
  "version": 2,
  "name": "chat-widget-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/chat-widget.js",
      "dest": "/embed/chat-widget.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {},
  "regions": ["iad1"]
}
